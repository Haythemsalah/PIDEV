{% extends 'base-front.html.twig' %}
{% block body %}
    <section class="classes-section spad">
    <div class="container">
    <div class="row">
        <div class="col-lg-12">

            <div class="section-title">
                <span>Ajouter une nouvelle Commandes  </span>
            </div>
</div>
{{ form_start(f,{'attr':{'novalidate':'novalidate'}})}}
<div class="container h-100">
    <div class="row align-items-center h-100">
        <table class="table table-dark">
            <tbody>
            <div class="container h-100">
                <div class="row align-items-center h-100">

                    <tr>
                        <th scope="row">{{ form_label(f.nom) }}</th>
                        <td>{{ form_widget(f.nom) }} </td>
                        <td>{{ form_errors(f.nom) }}</td>
                    </tr>

                    <tr>
                        <th scope="row">{{ form_label(f.prenom) }}</th>
                        <td>{{ form_widget(f.prenom) }} </td>
                        <td>{{ form_errors(f.prenom )}}</td>
                    </tr>

                    <tr>
                        <th scope="row">{{ form_label(f.quantite ) }}</th>
                        <td>{{ form_widget(f.quantite ) }}</td>
                        <td>{{ form_errors(f.quantite ) }}</td>
                    </tr>
                </div>
            </div>
            <tr>
                <th scope="row">{{ form_label(f.prix ) }}</th>
                <td>{{ form_widget(f.prix ) }} </td>
                <td>{{ form_errors(f.prix ) }}</td>
            </tr>
            <tr>
                <th scope="row"> {{ form_label(f.titre ) }}</th>
                <td>{{ form_widget(f.titre ) }} </td>
                <td>{{ form_errors(f.titre) }}</td>
            </tr>
            <tr>
                <th scope="row">{{ form_label(f.Total) }}</th>
                <td>{{ form_widget(f.Total) }} </td>
                <td>{{ form_errors(f.Total) }}</td>
            </tr>
<tr>
<th scope="row">{{ form_label(f.modepay) }}</th>
<td>{{ form_widget(f.modepay) }} </td>
<td>{{ form_errors(f.modepay) }}</td>
</tr>
            <tr>
                <th scope="row">{{ form_label(f.Date) }}</th>
                <td>{{ form_widget(f.Date) }} </td>
                <td>{{ form_errors(f.Date) }}</td>
            </tr>
            </tbody>
        </table>
        <div class="container h-100">
            <div class="row align-items-center h-100">
                <button class="btn btn-outline-info">Ajout</button>
            </div>
        </div>
        {{ form_end(f) }}
        {% endblock body %}