{% extends 'base-back.html.twig' %}
{% block body %}
<h1>Update Panniers</h1>
{{ form_start(f) }}
<div class="mx-auto" style="width: 500px;">
    <div class="font-italic">

        <h4 class="text-danger">Modifier  Panniers</h4>
    </div>
</div>
{{ form_start(f) }}
<div class="container h-100">
    <div class="row align-items-center h-100">
        <table class="table">
            <tbody>
            <div class="container h-100">
                <div class="row align-items-center h-100">

                    <tr>
                        <th scope="row">{{ form_label(f.nom) }}</th>
                        <td>{{ form_widget(f.nom) }}</td>
                        <td>{{ form_errors(f.nom) }}</td>
                    </tr>
                </div>
            </div>
            <tr>
                <th scope="row">{{ form_label(f.etat ) }}</th>
                <td>{{ form_widget(f.etat) }} </td>
                <td>{{ form_errors(f.etat) }}</td>
            </tr>
            <tr>
                <th scope="row">{{ form_label(f.dateexp	 ) }}</th>
                <td>{{ form_widget(f.dateexp) }} </td>
                <td>{{ form_errors(f.dateexp) }}</td>
            </tr>
            <tr>
                <th scope="row">{{ form_label(f.categorie ) }}</th>
                <td>{{ form_widget(f.categorie) }} </td>
                <td>{{ form_errors(f.categorie) }}</td>
            </tr>
            </tbody>
        </table>
        <div class="container h-100">
            <div class="row align-items-center h-100">
                <button class="btn btn-outline-info">Modifier</button>
            </div>
        </div>

        {{ form_end(f) }}
        {% endblock body %}

