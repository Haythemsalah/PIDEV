{% extends 'base-front.html.twig' %}
{% block body %}
    <section class="classes-section spad">
    <div class="container">
    <div class="row">
    <div class="col-lg-12">
        <div class="section-title">
            <span>Ajouter une nouvelle pannier </span>
    </div>
    {{ form_start(f,{'attr':{'novalidate':'novalidate'}})}}
    <div class="container h-100">
    <div class="row align-items-center h-100">

    <table class="table table-dark">
        <tbody>
        <div class="container h-100">
            <div class="row align-items-center h-100">
                <div class="ts_text">
                    <tr>
                        <th scope="row">{{ form_label(f.nom) }}</th>
                        <td>{{ form_widget(f.nom) }}</td>
                        <td>{{ form_errors(f.nom) }}</td>
                    </tr>
                </div>
            </div>
            <tr>
                <th scope="row">{{ form_label(f.etat ) }}</th>
                <td>{{ form_widget(f.etat) }} </td>
                <td>{{ form_errors(f.etat) }}</td>
            </tr>
            <tr>
                <th scope="row">{{ form_label(f.dateexp	 ) }}</th>
                <td>{{ form_widget(f.dateexp) }} </td>
                <td>{{ form_errors(f.dateexp) }}</td>
            </tr>
            <tr>
                <th scope="row">{{ form_label(f.categorie ) }}</th>
                <td>{{ form_widget(f.categorie) }} </td>
                <td>{{ form_errors(f.categorie) }}</td>
            </tr>
            </tbody>
        </table>
        <div class="container h-100">
            <div class="row align-items-center h-100">
                <button class="btn btn-outline-info">Ajout</button>
            </div>
        </div>
    </div>
        {{ form_end(f) }}
        {% endblock body %}